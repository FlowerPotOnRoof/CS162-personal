{
    "configurations": [
        {
            "name": "pintos",
            "includePath": [
                "${workspaceFolder}/**",
                "/usr/include",
                "/usr/include/x86_64-linux-gnu",
                "/usr/lib/gcc/x86_64-linux-gnu/11/include"
            ],
            "defines": [
                "USERPROG",
                "THREADS",
                "FILESYS"
            ],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c11",
            "intelliSenseMode": "linux-gcc-x64",
            "configurationProvider": "ms-vscode.makefile-tools",
            "compilerArgs": [
                "-nostdinc",
                "-I../..",
                "-I../../lib",
                "-I../../lib/kernel",
                "-Wall",
                "-W",
                "-Wstrict-prototypes",
                "-Wmissing-prototypes",
                "-Wsystem-headers"
            ]
        }
    ],
    "version": 4
}